{
  "i18n": {
    "cache": {
      "namespace": "{{DOOZR_NAMESPACE_FLAT}}.cache.i18n"
    },
    "default": {
      "locale": "en-us",
      "language": "en",
      "country": "us",
      "weight": 1,
      "available": [
        "de-de",
        "en-gb",
        "en-us"
      ],
      "namespace": "default"
    },
    "path": "{{DOOZR_APP_ROOT}}Data/Private/I18n/",
    "translator": {
      "interface": "Text"
    },
    "user": {
      "storages": [
        "session",
        "cookie"
      ],
      "identifier": "{{DOOZR_NAMESPACE_FLAT}}.i18n",
      "lifetime": 86400
    }
  }
}
