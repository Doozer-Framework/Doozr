                "tags": "ul,ol,li,a[href],font[size],span[style]",
                "fields": "POST.news"
            }
        }
    },
    "transmission": {
        "gzip": {
            "enabled": false
        },
        "ssl": {
            "force": false
        }
    },
    "debug": {
        "enabled": true
