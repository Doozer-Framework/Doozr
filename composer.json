{
  "name": "clickalicious/doozr",
  "description": "Doozr - The lightweight PHP-Framework for high-performance websites.",
  "authors": [
    {
      "name": "Benjamin Carl",
      "email": "opensource@clickalicious.de",
      "homepage": "https://www.clickalicious.de",
      "role": "Developer"
    }
  ],
  "require": {
      "php": ">=5.5.0",
      "doctrine/couchdb": "dev-master",
      "doctrine/couchdb-odm": "dev-master",
      "doctrine/dbal": "2.4.*",
      "doctrine/orm": "2.4.*",
      "doctrine/annotations": "1.2.*",
      "symfony/console": "2.6.*",
      "firephp/firephp-core": "0.4.*",
      "phptal/phptal": "1.3.*",
      "filp/whoops": "1.1.*",
      "maximebf/debugbar": "1.10.*",
      "raulfraile/ladybug": "1.0.*",
      "rhumsaa/uuid": "2.8.*",
      "moontoast/math": "1.1.*",
      "wp-cli/php-cli-tools": "0.10.*",
      "donatj/flags": "1.1.*",
      "clickalicious/memcached.php": "~0.4",
      "clickalicious/rng": "~0.1",
      "nikic/fast-route": "0.4.*",
      "nesbot/carbon": "1.18.*",
      "mrclay/minify": "2.2.*",
      "sepia/po-parser": "4.2.*",
      "psr/http-message": "1.0.*",
      "stack/callable-http-kernel": "1.0.*",
      "relay/relay": "1.*@dev",
      "friendsofphp/uprofiler": "dev-master"
  },
  "require-dev": {
    "raulfraile/ladybug": "1.0.*",
    "satooshi/php-coveralls": "dev-master",
    "phpunit/phpunit": "4.5.*"
  },
  "autoload": {
      "psr-0": {
          "Doozr": "lib/"
      }
  },
  "include-path": ["lib/"],
  "minimum-stability": "dev",
  "keywords": [
    "framework",
    "mvp",
    "mvc",
    "doozr",
    "REST",
    "clickalicious"
  ],
  "homepage": "https://www.clickalicious.de/",
  "license": [
    "The BSD License"
  ],
  "support": {
    "email": "opensource@clickalicious.de",
    "issues": "https://github.com/clickalicious/Doozr/issues",
    "wiki": "https://github.com/clickalicious/Doozr/wiki"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/clickalicious/Doozr.git"
    }
  ],
  "config" : {
    "bin-dir" : "bin"
  },
  "extra" : {
    "doozr-app-dir" : "app",
    "doozr-web-dir" : "web"
  }
}
