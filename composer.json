{
  "name": "clickalicious/doozr",
  "description": "Doozr - The lightweight PHP-Framework for high-performance websites.",
  "type": "project",
  "authors": [
    {
      "name": "Benjamin Carl",
      "email": "opensource@clickalicious.de",
      "homepage": "https://www.clickalicious.de",
      "role": "Lead"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.6.0",
    "ext-mbstring": "*",
    "clickalicious/cachingmiddleware": "^1.1",
    "clickalicious/memcached.php": "^1.0",
    "clickalicious/rng": "^1.1",
    "doctrine/dbal": "^2.5",
    "doctrine/orm": "^2.5",
    "doctrine/instantiator": "^1.0",
    "doctrine/annotations": "^1.2",
    "symfony/console": "^2.8",
    "symfony/polyfill-mbstring": "^1.1",
    "hautelook/phpass": "^1.0",
    "wp-cli/php-cli-tools": "^0.11.1",
    "donatj/flags": "^1.1",
    "nikic/fast-route": "^0.8.0",
    "maximebf/debugbar": "^1.11",
    "firephp/firephp-core": "^0.4.0",
    "filp/whoops": "^2.1",
    "ramsey/uuid": "^3.3",
    "moontoast/math": "^1.1",
    "phptal/phptal": "^1.3",
    "mrclay/minify": "^2.3",
    "sepia/po-parser": "^4.2",
    "psr/http-message": "^1.0",
    "psr/log": "^1.0",
    "lusitanian/oauth": "^0.8.9",
    "nesbot/carbon": "^1.21",
    "enygma/expose": "^2.4",
    "ircmaxell/password-compat": "^1.0",
    "relay/relay": "^1.1",
    "relay/middleware": "^1.0",
    "vlucas/phpdotenv": "^2.2",
    "shrikeh/teapot": "^1.0",
    "cocur/slugify": "^2.1",
    "guzzlehttp/guzzle": "^6.2",
    "league/event": "^2.1",
    "gpupo/cache": "^1.2",
    "danielstjules/stringy": "^2.3",
    "jbroadway/urlify": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "fzaninotto/faker": "^1.5",
    "symfony/var-dumper": "^2.8",
    "friendsofphp/uprofiler": "dev-master as 1.0.x-dev"
  },
  "autoload": {
    "psr-0": {
      "Doozr": "src/"
    }
  },
  "include-path": [
    "src/"
  ],
  "keywords": [
    "framework",
    "mvp",
    "mvc",
    "doozr",
    "REST",
    "clickalicious"
  ],
  "homepage": "https://www.clickalicious.de/",
  "license": "BSD-3-Clause",
  "support": {
    "email": "opensource@clickalicious.de",
    "issues": "https://github.com/clickalicious/Doozr/issues",
    "wiki": "https://github.com/clickalicious/Doozr/wiki"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/clickalicious/Doozr.git"
    }
  ],
  "config": {
    "bin-dir": "bin",
    "use-include-path": true,
    "preferred-install": "dist"
  },
  "extra": {
    "doozr-app-dir": "app",
    "doozr-web-dir": "web"
  }
}
