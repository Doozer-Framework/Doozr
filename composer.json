{
  "name": "clickalicious/doozr",
  "description": "DoozR - The lightweight MVP PHP-Framework for high-performance websites.",
  "authors": [
    {
      "name": "Benjamin Carl",
      "email": "opensource@clickalicious.de",
      "homepage": "http://www.clickalicious.de",
      "role": "Developer"
    }
  ],
  "require": {
      "doctrine/couchdb": "dev-master",
      "doctrine/couchdb-odm": "dev-master",
      "doctrine/dbal": "2.5.*@dev",
      "doctrine/orm": "2.5.*@dev",
      "doctrine/annotations": "1.3.*@dev",
      "symfony/console": ">=2.0",
      "firephp/firephp-core": "dev-master",
      "phptal/phptal": "1.3.0",
      "filp/whoops": "1.2.*@dev",
      "maximebf/debugbar": "1.*",
      "raulfraile/ladybug": "dev-master",
      "rhumsaa/uuid": "~2.7",
      "moontoast/math": "dev-master",
      "wp-cli/php-cli-tools": "dev-master",
      "donatj/flags": "1.*",
      "clickalicious/memcached.php": "~0.4",
      "nikic/fast-route": "dev-master"
  },
  "autoload": {
      "psr-0": {
          "DoozR": "Framework/"
      }
  },
  "include-path": ["Framework/"],
  "minimum-stability": "dev",
  "keywords": [
    "framework",
    "mvp",
    "mvc",
    "doozr",
    "REST",
    "clickalicious"
  ],
  "homepage": "http://doozr.clickalicious.de/",
  "license": [
    "The BSD License"
  ],
  "support": {
    "email": "opensource@clickalicious.de",
    "issues": "https://github.com/clickalicious/DoozR/issues",
    "wiki": "https://github.com/clickalicious/DoozR/wiki"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/clickalicious/DoozR.git"
    }
  ],
  "config" : {
    "bin-dir" : "bin"
  },
  "extra" : {
    "doozr-app-dir" : "app",
    "doozr-web-dir" : "web"
  }
}
