{
  "name": "clickalicious/doozr",
  "description": "Doozr - The lightweight PHP-Framework for high-performance websites.",
  "authors": [
    {
      "name": "Benjamin Carl",
      "email": "opensource@clickalicious.de",
      "homepage": "https://www.clickalicious.de",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "clickalicious/cachingmiddleware": "~1.0",
    "clickalicious/memcached.php": "~0.4",
    "clickalicious/rng": "~0.1",
    "doctrine/dbal": "~2.5.1",
    "doctrine/orm": "~2.5.0",
    "doctrine/instantiator": "~1.0.5",
    "doctrine/annotations": "~1.2.6",
    "symfony/var-dumper": "~2.7.2",
    "symfony/console": "~2.7.1",
    "wp-cli/php-cli-tools": "~0.10.4",
    "donatj/flags": "~1.1.0",
    "nikic/fast-route": "~0.6.0",
    "maximebf/debugbar": "~1.10.4",
    "firephp/firephp-core": "~0.4.0",
    "filp/whoops": "~1.1.7",
    "ramsey/uuid": "~2.8.1",
    "moontoast/math": "~1.1.0",
    "phptal/phptal": "~1.3.0",
    "mrclay/minify": "~2.2.1",
    "sepia/po-parser": "~4.2.1",
    "psr/http-message": "~1.0.0",
    "psr/log": "~1.0.0",
    "lusitanian/oauth": "~0.3.5",
    "nesbot/carbon": "~1.20.0",
    "enygma/expose": "~2.3.0",
    "ircmaxell/password-compat": "~1.0.4",
    "relay/relay": "1.*@dev",
    "relay/middleware": "1.*@dev",
    "vlucas/phpdotenv": "~2.0.1",
    "friendsofphp/uprofiler": "dev-master as 1.0.x-dev",
    "shrikeh/teapot": "~1.0.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8.9",
    "satooshi/php-coveralls": "dev-master as 1.0.x-dev"
  },
  "autoload": {
    "psr-0": {
      "Doozr": "src/"
    }
  },
  "include-path": [
    "src/"
  ],
  "minimum-stability": "dev",
  "keywords": [
    "framework",
    "mvp",
    "mvc",
    "doozr",
    "REST",
    "clickalicious"
  ],
  "homepage": "https://www.clickalicious.de/",
  "license": [
    "BSD-2-Clause"
  ],
  "support": {
    "email": "opensource@clickalicious.de",
    "issues": "https://github.com/clickalicious/Doozr/issues",
    "wiki": "https://github.com/clickalicious/Doozr/wiki"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/clickalicious/Doozr.git"
    }
  ],
  "config": {
    "bin-dir": "bin",
    "preferred-install": "dist"
  },
  "extra": {
    "doozr-app-dir": "app",
    "doozr-web-dir": "web",
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
