{
  "name": "clickalicious/doozr",
  "description": "Doozr - The lightweight PHP-Framework for high-performance websites.",
  "type": "project",
  "authors": [
    {
      "name": "Benjamin Carl",
      "email": "opensource@clickalicious.de",
      "homepage": "https://www.clickalicious.de",
      "role": "Lead"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.6.0",
    "ext-mbstring": "*",
    "clickalicious/cachingmiddleware": "~1.1.0",
    "clickalicious/memcached.php": "~0.4.0",
    "clickalicious/rng": "~1.1.0",
    "doctrine/dbal": "~2.5.4",
    "doctrine/orm": "~2.5.4",
    "doctrine/instantiator": "~1.0.5",
    "doctrine/annotations": "~1.2.7",
    "symfony/console": "~2.8.4",
    "symfony/polyfill-mbstring": "~1.1.1",
    "hautelook/phpass": "~1.0.0",
    "wp-cli/php-cli-tools": "~0.11.1",
    "donatj/flags": "~1.1.0",
    "nikic/fast-route": "~0.8.0",
    "maximebf/debugbar": "~1.11.1",
    "firephp/firephp-core": "~0.4.0",
    "filp/whoops": "~2.1.2",
    "ramsey/uuid": "~3.3.0",
    "moontoast/math": "~1.1.0",
    "phptal/phptal": "~1.3.0",
    "mrclay/minify": "~2.3.0",
    "sepia/po-parser": "~4.2.1",
    "psr/http-message": "~1.0.0",
    "psr/log": "~1.0.0",
    "lusitanian/oauth": "~0.8.9",
    "nesbot/carbon": "~1.21.0",
    "enygma/expose": "~2.4.0",
    "ircmaxell/password-compat": "~1.0.4",
    "relay/relay": "1.*@dev",
    "relay/middleware": "1.*@dev",
    "vlucas/phpdotenv": "~2.2.0",
    "friendsofphp/uprofiler": "dev-master as 1.0.x-dev",
    "shrikeh/teapot": "~1.0.2",
    "cocur/slugify": "~2.1.1",
    "guzzlehttp/guzzle": "~6.2.0",
    "league/event": "~2.1.2",
    "gpupo/cache": "~1.2.2",
    "danielstjules/stringy": "~2.3.1",
    "jbroadway/urlify": "1.0.7-stable"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8.9",
    "fzaninotto/faker": "~1.5.0",
    "symfony/var-dumper": "~2.8.4"
  },
  "autoload": {
    "psr-0": {
      "Doozr": "src/"
    }
  },
  "include-path": [
    "src/"
  ],
  "keywords": [
    "framework",
    "mvp",
    "mvc",
    "doozr",
    "REST",
    "clickalicious"
  ],
  "homepage": "https://www.clickalicious.de/",
  "license": "BSD-3-Clause",
  "support": {
    "email": "opensource@clickalicious.de",
    "issues": "https://github.com/clickalicious/Doozr/issues",
    "wiki": "https://github.com/clickalicious/Doozr/wiki"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/clickalicious/Doozr.git"
    }
  ],
  "config": {
    "bin-dir": "bin",
    "use-include-path": true,
    "preferred-install": "dist"
  },
  "extra": {
    "doozr-app-dir": "app",
    "doozr-web-dir": "web"
  }
}
