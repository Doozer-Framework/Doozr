{
  "name": "clickalicious/rng",
  "description": "Random number generator for PHP",
  "authors": [
    {
      "name": "Benjamin Carl",
      "email": "opensource@clickalicious.de",
      "homepage": "https://www.clickalicious.de",
      "role": "Developer"
    }
  ],
  "require": {
      "php": ">=5.3.0"
  },
  "require-dev": {
      "satooshi/php-coveralls": "dev-master",
      "phpunit/phpunit": "4.5.*"
  },
  "autoload": {
      "psr-0": {
          "Clickalicious": "lib/"
      }
  },
  "include-path": ["lib/"],
  "minimum-stability": "dev",
  "keywords": [
    "RNG",
    "PRNG",
    "Random",
    "Randomize",
    "Number",
    "clickalicious"
  ],
  "homepage": "https://github.com/clickalicious/Rng",
  "license": [
    "The BSD License"
  ],
  "support": {
    "email": "opensource@clickalicious.de",
    "issues": "https://github.com/clickalicious/Rng/issues",
    "wiki": "https://github.com/clickalicious/Rng/wiki"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/clickalicious/Rng.git"
    }
  ],
  "config" : {
    "bin-dir" : "bin"
  }
}
