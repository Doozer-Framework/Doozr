{
  "name": "clickalicious/doozr",
  "description": "Doozr - The lightweight PHP-Framework for high-performance websites.",
  "authors": [
    {
      "name": "Benjamin Carl",
      "email": "opensource@clickalicious.de",
      "homepage": "https://www.clickalicious.de",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "doctrine/couchdb": "dev-master",
    "doctrine/couchdb-odm": "dev-master",
    "doctrine/dbal": "2.5.*",
    "doctrine/orm": "2.5.*",
    "doctrine/annotations": "1.2.*",
    "raulfraile/ladybug": "1.0.*",
    "maximebf/debugbar": "1.10.*",
    "symfony/console": "2.6.*",
    "firephp/firephp-core": "0.4.*",
    "phptal/phptal": "1.3.*",
    "filp/whoops": "1.1.*",
    "ramsey/uuid": "2.8.*",
    "moontoast/math": "1.1.*",
    "wp-cli/php-cli-tools": "0.10.*",
    "donatj/flags": "1.1.*",
    "clickalicious/memcached.php": "~0.4",
    "clickalicious/rng": "~0.1",
    "nikic/fast-route": "0.4.*",
    "mrclay/minify": "2.2.*",
    "sepia/po-parser": "4.2.*",
    "psr/http-message": "1.0.*",
    "psr/log": "1.0.*",
    "lusitanian/oauth": "0.3.*",
    "nesbot/carbon": "1.18.*",
    "enygma/expose": "2.3.*",
    "ircmaxell/password-compat": "1.0.*",
    "relay/relay": "1.*@dev",
    "relay/middleware": "1.*@dev",
    "vlucas/phpdotenv": "2.0.*",
    "friendsofphp/uprofiler": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "4.5.*",
    "satooshi/php-coveralls": "dev-master",
  },
  "autoload": {
    "psr-0": {
      "Doozr": "lib/"
    }
  },
  "include-path": [
    "lib/"
  ],
  "minimum-stability": "dev",
  "keywords": [
    "framework",
    "mvp",
    "mvc",
    "doozr",
    "REST",
    "clickalicious"
  ],
  "homepage": "https://www.clickalicious.de/",
  "license": [
    "The BSD License"
  ],
  "support": {
    "email": "opensource@clickalicious.de",
    "issues": "https://github.com/clickalicious/Doozr/issues",
    "wiki": "https://github.com/clickalicious/Doozr/wiki"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/clickalicious/Doozr.git"
    }
  ],
  "config": {
    "bin-dir": "bin"
  },
  "extra": {
    "doozr-app-dir": "app",
    "doozr-web-dir": "web"
  }
}
